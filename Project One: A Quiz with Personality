}
	
	
	function findValue(inputs) {
		var checked;
		for (var i=0; i<inputs.length; i++) {
			if (inputs[i].checked) {
				//alert(inputs[i].value);
				checked = inputs[i].value;
				return(checked);
			}
		}
		if(!checked) {
			alert('Choose an answer.');
		}
	}
	function displayCharacter(situation) {
		var image = document.createElement('img');
		image.className = 'character';
		var source = document.getElementById('resultParagraph');
		var text = 'You are a(n) ' + situation + ' character!';
		
		switch(situation) {
			case 'Protagonist':
				image.src = 'https://s-media-cache-ak0.pinimg.com/736x/6d/81/8d/6d818dd5abdf139070378a11c1120b00.jpg';
				break;
			case 'Antagonist':
				image.src = 'https://s-media-cache-ak0.pinimg.com/736x/76/3c/99/763c99967a83f25a790bf6c1a85851f3.jpg';
				break;
			case 'Foil':
				image.src = 'http://i715.photobucket.com/albums/ww151/tuvok1983/Nikinek_SpockKirk.jpg';
				break;
			case 'Love interest':
				image.src = 'http://www.scifiheaven.net/wp-content/uploads/2013/12/1-tauriel-690x423.jpg';
				break;
		}
		source.appendChild(image);
		source.innerHTML += '<br>' + text;
	}
	
	function displayCharacter2(situation) {
		var image = document.createElement('img');
		image.className = 'character';
		var source = document.getElementById('resultParagraph2');
		var text = 'You are a(n): ' + situation;
		
		switch(situation) {
			case 'Protagonist':
				image.src = '';
				break;
			case 'Antagonist':
				image.src ='';
				break;
			case 'Foil':
				image.src ='';
				break;
			case 'Love interest':
				image.src ='';
				break;
		}
		source.appendChild(image);
		source.innerHTML += '<br>' + text;
	}
	window.onload = init;
</script>
