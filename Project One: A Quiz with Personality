<script>
	var myCharacters = ['Protagonist', 'Antagonist', 'Foil', 'Love interest'];
	var points = [0,0,0,0];
	
	function init() {
		var button = document.getElementById('quizButton');
		button.onclick = checkQuiz;
	}
	
	function checkQuiz() {
		var situation;
		var situation2;
		var situation3;
		var situation4;
		var situation5;
		var situation6;
		var situation7;
		var situation8;
		var situation9;
		var situation10;
		var inputs = document.getElementsByName('situation');
		situation = findValue(inputs);
		switch(situation) {
			case 'Instigator':
				points[1]++;
				break;
			case 'Main':
				points[0]++;
				break;
			case 'Foil':
				points[2]++;
				break;
			case 'Heroine':
				points[3]++;
				break;
			}
